{
  "version": 2,
  "builds": [
    {
      "src": "api/entry.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/entry",
      "dest": "/api/entry.js"
    },
    {
      "src": "/api/entry/(.*)",
      "dest": "/api/entry.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
